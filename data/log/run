#!/bin/execlineb -P
redirfd -w 2 /dev/null fdmove -c 2 1
emptyenv -p s6-envdir ../env
multisubstitute {
	import -i USER
	import -i HOME
}

s6-envuidgid $USER
backtick -i GIDLIST {
	multisubstitute {
		import -i GID
		import -i GIDLIST
	}
	printf %s,%s $GID $GIDLIST
}
s6-applyuidgid -Uz
s6-log t ${HOME}/.user:log
